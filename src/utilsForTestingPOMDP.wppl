var getMakeAgentFunction = function(beliefOrBeliefDelay){
  if ( _.isUndefined(beliefOrBeliefDelay) || 
       beliefOrBeliefDelay == 'beliefDelay'){
    return makeBeliefDelayAgent;
  } else {
    return makeBeliefAgent;
  }
};

var getSimulateFunction = function(beliefOrBeliefDelay){
  if ( _.isUndefined(beliefOrBeliefDelay) || 
       beliefOrBeliefDelay == 'beliefDelay'){
    return simulateBeliefDelayAgent;
  } else {
    return simulateBeliefAgent;
  }
};


var _getPriorBelief = function(perceivedTotalTime, startingLocation, latentStateSampler, noManifest){
  if (noManifest){
    var startManifestState = {timeLeft: perceivedTotalTime, loc: startingLocation, terminateAfterAction: false};
    return Enumerate( function(){
      var latentState = latentStateSampler();
      return buildState(startManifestState, latentState);
    });
  } else {
    return Enumerate(latentStateSampler);
  }
};

var getPriorBeliefGridworld = function(startManifestState, latentStateSampler, noManifest){
  if (noManifest){
    return Enumerate( function(){
      var latentState = latentStateSampler();
      return buildState(startManifestState, latentState);
    });
  } else {
    return Enumerate(latentStateSampler);
  }
};





