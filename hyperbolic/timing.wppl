var start = { 
  loc : [3,0],
  terminateAfterAction : false,
   
  timeLeft : 13
};

var donutUtility = makeDonutUtility(2, {
    'Donut' : [1, -1],
    'Veg'   : [-1, 2],
    'Noodle': [0, 0]
});

var world = makeDonutWorld2({ big : true});
var agent = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'sophisticated' 
    }, world);
var sophisticated = function () { 
  var f = agent.agent;
  var trajectory = f(start);
};

var agent2 = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'naive' 
    }, world);

var naive = function () { 
  var f = agent2.agent;
  var r = simulate(start, world, agent2, 9, 'states');
};

print("sophisticated " + timeit(sophisticated).runtimeInMilliseconds);
print("naive         " + timeit(naive).runtimeInMilliseconds);
