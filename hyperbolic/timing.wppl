var start = { 
  loc : [2,0],
  dead : false,
  timeLeft : 9,
  digest : 4
};

var donutUtility = makeDonutUtility(4, {
    'Donut' : [1, 0, 0, 0],
    'Veg'   : [0, 0, 0, 3.5],
    'Noodle': [0, 0, 0, 0]});

var world = makeDonutWorld2({ bottomLeftNook : '#' });
var agent = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'sophisticated' 
    }, world);
var sophisticated = function () { 
  var f = agent.agent;
  var trajectory = f(start);
};

var world2 = makeDonutWorld2({ bottomLeftNook : { name : 'Donut'}  });
var agent2 = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'naive' 
    }, world2);

var naive = function () { 
  var f = agent2.agent;
  var r = simulate(start, world2, agent2, 9, 'states');
};

print("sophisticated " + timeit(sophisticated).runtimeInMilliseconds);
print("naive         " + timeit(naive).runtimeInMilliseconds);
