var start = { 
  loc : [2,0],
  dead : false,
  digest : 4, 
  timeLeft : 12
};

var donutUtility = makeDonutUtility(4, {
    'Donut' : [1, 0, 0, 0],
    'Veg'   : [0, 0, 0, 3.5],
    'Noodle': [0, 0, 0, 0]
});

var world = makeDonutWorldExtended({ bottomLeftNook : '#' });
var agent = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'sophisticated' 
    }, world);
mdpSim(start, world, agent, 12);

var world2 = makeDonutWorldExtended({ bottomLeftNook : { name : 'Donut'}  });
var agent2 = makeHyperbolicDiscounter(
    { utility : donutUtility,
      alpha : 500, 
      discount : 1, 
      sophisticatedOrNaive  : 'naive' 
    }, world2);
mdpSim(start, world2, agent2, 12);
